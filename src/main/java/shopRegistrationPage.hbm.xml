<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
<class name="bean.ShopRegistration" table="registeredshops">
<id name="id">
<generator class="increment"></generator>
</id>
<property name="shopName"></property>
<property name="shopCategoryOptions"></property>
<property name="stateOptions"></property>
<property name="item"></property>
<property name="shopCity"></property>
<property name="shopZip"></property>
<property name="addressLine"></property>
<property name="latitude"></property>
<property name="longitude"></property>
<property name="phnNumber"></property>
<property name="shopPhnNumber"></property>
<property name="ownerNumber"></property>
<property name="ownerName"></property>
<property name="ownerMiddleName"></property>
<property name="ownerLastName"></property>
<property name="ownerEmailID"></property>
<property name="shopPassword"></property>
</class>
</hibernate-mapping>